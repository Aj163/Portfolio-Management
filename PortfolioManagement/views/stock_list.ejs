<!DOCTYPE html>
<html>
    <head>
        <title>
        	Portfolio
        </title>
        <style>
                body{background: #003300; font-family: ubuntu; color: #ffffff; padding: 30px;}
                h1{font-size: 48px; text-transform: uppercase; letter-spacing: 2px; text-align: center;}
                p{font-size: 32px; text-align: center;}
                h6{font-size: 25px; text-align: center;}
                li{font-size: 20px; text-align: center;}
        </style>
    </head>

    <body>
        <p style="text-align: right;"><a href="/logout" style="color: white">
            Logout</a></p>
        <h1>List of all Stocks!</h1>
        <div id="link" style="text-align: center;">
        <% data.forEach(function(item) { %>
            <li><a href="/share/<%= item.Symbol %>" style="color: white; font-size: 20px;">
                <%= item.ShareName %></a></li>
        <% }); %>
        </div>
    </body>
</html>
